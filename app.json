{
  "expo": {
    "name": "nomadeApp",
    "slug": "nomadeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/shopping-cart.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/shopping-cart.gif",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.nomadeApp",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "package": "com.nomadeApp",
        "versionCode": 1,
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["ACCESS_FINE_LOCATION", "CAMERA", "WRITE_EXTERNAL_STORAGE"],
      "config": {
        "googleMaps": {
          "apiKey": "asdasdasd"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
